<?xml version="1.0" encoding="UTF-8"?>
<bindings xmlns="http://java.sun.com/xml/ns/jaxb" version="2.0"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">
	<bindings schemaLocation="eXTra-components-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.components" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="eXTra-logging-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.logging" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="eXTra-plugins-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.plugins" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="eXTra-request-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.request" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="eXTra-response-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.response" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="extra-error-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.service" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="eXTra-messages-1.xsd">
		<schemaBindings>
			<package name="de.drv.dsrv.spoc.extra.v1_3.jaxb.messages" />
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="extra-error-1.xsd" node="//xs:simpleType[@name='ExtraErrorReasonType']">
	    <typesafeEnumClass name="ExtraErrorReasonType">
            <typesafeEnumMember value="http://www.extra-standard.de/reason/SERVICE_TEMPORARILY_UNAVAILABLE" name="SERVICE_TEMPORARILY_UNAVAILABLE" />
            <typesafeEnumMember value="http://www.extra-standard.de/reason/SERVICE_PERMANENTLY_UNAVAILABLE" name="SERVICE_PERMANENTLY_UNAVAILABLE" />
            <typesafeEnumMember value="http://www.extra-standard.de/reason/SERVICE_FAILURE" name="SERVICE_FAILURE" />
            <typesafeEnumMember value="http://www.extra-standard.de/reason/INVALID_REQUEST" name="INVALID_REQUEST" />
            <typesafeEnumMember value="http://www.extra-standard.de/reason/UNSPECIFIED" name="UNSPECIFIED" />
	    </typesafeEnumClass>
	</bindings>
	<bindings schemaLocation="eXTra-components-1.xsd" node="//xs:simpleType[@name='SupportedVersionsType']">
	    <typesafeEnumClass name="SupportedVersionsType">
            <typesafeEnumMember value="1.0" name="VERSION_1_0" />
            <typesafeEnumMember value="1.1" name="VERSION_1_1" />
            <typesafeEnumMember value="1.2" name="VERSION_1_2" />
            <typesafeEnumMember value="1.3" name="VERSION_1_3" />
	    </typesafeEnumClass>
	</bindings>
	<bindings schemaLocation="eXTra-logging-1.xsd" node="//xs:simpleType[@name='LoggingVersionType']">
	    <typesafeEnumClass name="LoggingVersionType">
            <typesafeEnumMember value="1.0" name="VERSION_1_0" />
	    </typesafeEnumClass>
	</bindings>
	<bindings schemaLocation="eXTra-plugins-1.xsd" node="//xs:simpleType[@name='EndpointTypeType']">
	    <typesafeEnumClass name="EndpointTypeType" />
	</bindings>
	<bindings schemaLocation="eXTra-messages-1.xsd" node="//xs:simpleType[@name='ExtraLayerNamesType']">
	    <typesafeEnumClass name="ExtraLayerNamesType">
            <typesafeEnumMember value="http://www.extra-standard.de/layer/Transport" name="Transport" />
            <typesafeEnumMember value="http://www.extra-standard.de/layer/Package" name="Package" />
            <typesafeEnumMember value="http://www.extra-standard.de/layer/Message" name="Message" />
            <typesafeEnumMember value="http://www.extra-standard.de/layer/Application" name="Application" />
            <typesafeEnumMember value="http://www.extra-standard.de/layer/Delivery" name="Delivery" />
	    </typesafeEnumClass>
	</bindings>
	<bindings schemaLocation="eXTra-messages-1.xsd" node="//xs:simpleType[@name='StatusNamesType']">
	    <typesafeEnumClass name="StatusNamesType">
            <typesafeEnumMember value="http://www.extra-standard.de/status/ACCEPTED" name="ACCEPTED" />
            <typesafeEnumMember value="http://www.extra-standard.de/status/PROCESSING" name="PROCESSING" />
            <typesafeEnumMember value="http://www.extra-standard.de/status/COMPLETED" name="COMPLETED" />
            <typesafeEnumMember value="http://www.extra-standard.de/status/CONFIRMED" name="CONFIRMED" />
            <typesafeEnumMember value="http://www.extra-standard.de/status/FAILED" name="FAILED" />
	    </typesafeEnumClass>
	</bindings>
	<bindings schemaLocation="eXTra-messages-1.xsd" node="//xs:simpleType[@name='EventNamesType']">
	    <typesafeEnumClass name="EventNamesType">
            <typesafeEnumMember value="http://www.extra-standard.de/event/Default" name="Default" />
            <typesafeEnumMember value="http://www.extra-standard.de/event/SendData" name="SendData" />
            <typesafeEnumMember value="http://www.extra-standard.de/event/RequestData" name="RequestData" />
	    </typesafeEnumClass>
	</bindings>
</bindings>